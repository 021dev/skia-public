int return_loop_0()
{
	for (int i = 0;i < 10; ++i) 
	{
		if (i == 5) 
		{
			return i;
		}
	}
	return 0;
}
int continue_loop_0()
{
	int sum = 0;
	for (int i = 0;i < 10; ++i) 
	{
		if (i < 5) 
		{
			continue;
		}
		sum += i;
	}
	return sum;
}
int break_loop_0()
{
	int sum = 0;
	for (int i = 0;i < 10; ++i) 
	{
		if (i > 5) 
		{
			break;
		}
		sum += i;
	}
	return sum;
}
half4 main()
{
	return half4(half4(half(return_loop_0()), half(continue_loop_0()), half(break_loop_0()), 1.0));
}
